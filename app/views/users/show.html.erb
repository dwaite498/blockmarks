<h3><%= @user.email %></h3>
<ul>
    <li><%= link_to "Edit Profile", edit_user_registration_path %></li>
</ul>

<h3>My Topics</h3>
    <div class="wrapper">
    <% @topics.each do |f| %>
        <div class="topic-container">
            <h4><%=link_to f.title, topic_url(f) %><%= link_to "", topic_url(f), :remote => true, method: :delete, class: 'glyphicon glyphicon-remove delete_item' %></h4>
            <% f.bookmarks[0..4].each do |b| %>
                <p><%= b.url %></p>
        
        <% end %>
        </div>
    <% end %>
    </div>
    <h3>Liked Bookmarks</h3>
        <div class="wrapper">
            <div class="topic-container">
                <% @liked_bookmarks.each do |f| %>
                        <p><%= f.bookmark.url %></p>
                <% end %>
            </div>
        </div>
        